{
  "name": "mij-customer-mij",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "build:check": "tsc -b && vite build && npm run postbuild",
    "postbuild": "cp public/robots.txt dist/robots.txt",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy stg": "npm run build && aws s3 sync dist/ s3://stg-mij-customer-webapp --delete && aws cloudfront create-invalidation --distribution-id E2BF051CTUBS9Y --paths \"/*\"",
    "deploy prod": "npm run build && aws s3 sync dist/ s3://prd-mij-customer-webapp --delete && aws cloudfront create-invalidation --distribution-id E2A91SB6ISPQW2 --paths \"/*\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.50",
    "date-fns": "^4.1.0",
    "hls.js": "^1.6.13",
    "keen-slider": "^6.8.6",
    "path": "^0.12.7",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/history": "^4.7.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "lucide-react": "^0.536.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwind-variants": "^2.1.0",
    "tailwindcss": "^3.4.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.4",
    "vite-plugin-sitemap": "^0.8.2"
